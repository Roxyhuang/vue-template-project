{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-0"
  ],
  "plugins": ["transform-decorators-legacy","transform-runtime",["component", [{"libraryName": "mint-ui","style": true}]]],
  "env": {
    "test": {
      "presets": ["env", "stage-0"],
      "plugins": ["istanbul"]
    }
  }
}
